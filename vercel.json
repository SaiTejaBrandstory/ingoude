{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/banking-services",
      "dest": "/banking-services.html"
    },
    {
      "src": "/services",
      "dest": "/services.html"
    },
    {
      "src": "/financial-services",
      "dest": "/financial-services.html"
    },
    {
      "src": "/insurance-services",
      "dest": "/insurance-services.html"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    }
  ]
}

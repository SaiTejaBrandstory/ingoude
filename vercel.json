{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/banking-services",
      "dest": "/banking-services.html"
    },
    {
      "src": "/services",
      "dest": "/services.html"
    },
    {
      "src": "/financial-services",
      "dest": "/financial-services.html"
    },
    {
      "src": "/insurance-services",
      "dest": "/insurance-services.html"
    },
    {
      "src": "/contact-us",
      "dest": "/contact-us.html"
    },
    {
      "src": "/conversalq",
      "dest": "/conversalq.html"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    }
  ],
  "headers": [
    {
      "source": "(.*)\\.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" }
      ]
    },
    {
      "source": "/public/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "(.*)\\.(css|js|svg|png|webp|jpg|jpeg|gif)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
